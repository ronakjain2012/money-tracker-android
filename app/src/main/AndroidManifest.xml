<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ronak.myexpensetracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/et"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/et"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Gate_Validation"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="com.example.ronak.myexpenseDashboard" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".income"
            android:label="Income" />
        <activity
            android:name=".expense"
            android:label="Expense" />
        <activity
            android:name=".Category"
            android:label="Add Category" />
        <activity
            android:name=".setFirstPassword"
            android:label="Set PIN" />
        <activity android:name=".MonthWiseView" />
        <activity android:name=".AccountView" />
        <activity android:name=".AverageSummary" />
        <activity android:name=".FullStatement"></activity>
    </application>

</manifest>